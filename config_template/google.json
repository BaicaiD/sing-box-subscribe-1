{
  "log": {
    "disabled": false,
    "level": "debug",
    "output": "/data/adb/box/run/sing-box.log",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "proxyDns",
        "address": "tls://8.8.8.8",
        "detour": "proxy"
      },
      {
        "tag": "localDns",
        "address": "https://223.5.5.5/dns-query",
        "detour": "direct"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "domain": [
          "ghproxy.com",
          "cdn.jsdelivr.net",
          "testingcf.jsdelivr.net"
        ],
        "server": "localDns"
      },
      {
        "geosite": "category-ads-all",
        "server": "block"
      },
      {
        "outbound": "any",
        "server": "localDns",
        "disable_cache": true
      },
      {
        "geosite": "cn",
        "server": "localDns"
      },   
      {
        "clash_mode": "direct",
        "server": "localDns"
      },
      {
        "clash_mode": "global",
        "server": "proxyDns"
      },
      {
        "geosite": "geolocation-!cn",
        "server": "proxyDns"
      }
    ],
    "final": "localDns",
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "type": "tun",
      "inet4_address": "172.19.0.1/30",
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "sniff": true,
      "endpoint_independent_nat": false,
      "stack": "system",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2080
        }
      }
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "sniff": true,
      "users": []
    }
  ],
  "outbounds": [
    {
      "tag":"proxy",
      "type":"selector",
      "outbounds":[
        "auto",
        "direct",
        "{all}"
      ]
    },
    {
      "tag":"🤖 OpenAI",
      "type":"selector",
      "outbounds":[
        "AI 人工智能",
        "✈️ 其他节点"
      ],
      "default": "AI 人工智能"
    },
    {
      "tag":"🌌 Google",
      "type":"selector",
      "outbounds":[
        "{all}"
      ]
    },
    {
      "tag":"📺 Bilibili",
      "type":"selector",
      "outbounds":[
        "direct",
        "🇭🇰 香港节点",
        "🇹🇼 台湾节点"
      ]
    },
    {
      "tag":"😗 Tiktok",
      "type":"selector",
      "outbounds":[
        "🇭🇰 香港节点",
        "🇹🇼 台湾节点",
        "🇸🇬 狮城节点",
        "🇯🇵 日本节点",
        "🇺🇸 美国节点",
        "✈️ 其他节点",
        "direct"
      ]
    },
    {
      "tag":"🎵 Spotify",
      "type":"selector",
      "outbounds":[
        "🇯🇵 日本节点",
        "🇺🇸 美国节点",
        "✈️ 其他节点",
        "direct"
      ],
      "default": "🇯🇵 日本节点"
    },
    {
      "tag":"🌏 !cn",
      "type":"selector",
      "outbounds":[
        "🇭🇰 香港节点",
        "🇹🇼 台湾节点",
        "🇸🇬 狮城节点",
        "🇯🇵 日本节点",
        "🇺🇸 美国节点",
        "✈️ 其他节点",
        "direct"
      ]
    },
    {
      "tag":"🇭🇰 香港节点",
      "type":"selector",
      "outbounds":[
        "{all}",
        "proxy"
      ],
      "filter":[
        {"action":"include","keywords":["🇭🇰|HK|hk|香港|港|HongKong"]}
      ]
    },
    {
      "tag":"🇹🇼 台湾节点",
      "type":"selector",
      "outbounds":[
        "{all}",
        "proxy"
      ],
      "filter":[
        {"action":"include","keywords":["🇹🇼|TW|tw|台湾|臺灣|台|Taiwan"]}
      ]
    },
    {
      "tag":"🇸🇬 狮城节点",
      "type":"selector",
      "outbounds":[
        "{all}",
        "proxy"
      ],
      "filter":[
        {"action":"include","keywords":["🇸🇬|SG|sg|新加坡|狮|Singapore"]}
      ]
    },
    {
      "tag":"🇯🇵 日本节点",
      "type":"selector",
      "outbounds":[
        "{all}",
        "proxy"
      ],
      "filter":[
        {"action":"include","keywords":["🇯🇵|JP|jp|日本|日|Japan"]}
      ]
    },
    {
      "tag":"🇺🇸 美国节点",
      "type":"selector",
      "outbounds":[
        "{all}",
        "proxy"
      ],
      "filter":[
        {"action":"include","keywords":["🇺🇸|US|us|美国|美|United States"]}
      ]
    },
        {
      "tag":"AI 人工智能",
      "type":"selector",
      "outbounds":[
        "{all}",
        "proxy"
      ],
      "filter":[
        {"action":"include","keywords":["GPT|gpt|🇺🇸|US|us|美国|美|United States|🇯🇵|JP|jp|日本|日|Japan"]}
      ]
    },
    {
      "tag":"✈️ 其他节点",
      "type":"selector",
      "outbounds":[
        "{all}",
        "proxy"
      ],
      "filter":[
        {"action":"exclude","keywords":["🇭🇰|HK|hk|香港|香|🇹🇼|TW|tw|台湾|台|🇸🇬|SG|sg|新加坡|狮|🇯🇵|JP|jp|日本|日|🇺🇸|US|us|美国|美|网站|剩余|流量|过期|时间|期|地址"]}
      ]
    },
    {
      "tag":"🛑 AdBlock",
      "type":"selector",
      "outbounds":[
        "block",
        "direct"
      ]
    },
    {
      "tag":"auto",
      "type":"urltest",
      "outbounds":[
        "{all}"
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "5m",
      "tolerance": 50
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "geoip": {
      "download_url": "https://github.com/lyc8503/sing-box-rules/releases/latest/download/geoip.db",
      "download_detour": "direct"
    },
    "geosite": {
      "download_url": "https://github.com/lyc8503/sing-box-rules/releases/latest/download/geosite.db",
      "download_detour": "direct"
    },
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "network": "udp",
        "port": 443,
        "outbound": "block"
      },
      {
        "geosite": "category-ads-all",
        "outbound": "🛑 AdBlock"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "global",
        "outbound": "proxy"
      },
      {
        "domain": [
          "clash.razord.top",
          "yacd.metacubex.one",
          "yacd.haishan.me",
          "d.metacubex.one"
        ],
        "outbound": "direct"
      },
      {
        "geosite":"openai",
        "outbound":"🤖 OpenAI"
      },
      {
        "geoip":"google",
        "outbound":"🌌 Google"
      },
      {
        "geosite":["google","github"],
        "outbound":"🌌 Google"
      },
      {
        "geosite":"spotify",
        "outbound":"🎵 Spotify"
      },
      {
        "geosite":"bilibili",
        "outbound":"📺 Bilibili"
      },
      {
        "geosite":"tiktok",
        "outbound":"😗 Tiktok"
      },
      {
        "geosite":"geolocation-!cn",
        "outbound":"🌏 !cn"
      },
    ]
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "",
      "default_mode": "rule"
    },
  "cache_file": {
    "enabled": true,
    "store_fakeip": false
    }
  }
}
